<h3>All songs</h3>

<table>

  <thead>
    <tr>
      <th>Title</th>
      <th>Genre</th>
      <th>Artist</th>
      <th>Released?</th>
      <th>Release Year</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
        <tr>
          <td><%= link_to song.title, song %></td>
          <td><%= song.genre %></td>
          <td><%= song.artist_name %></td>
          <td><%= song.released %></td>
          <td><%= song.release_year %></td>
        </tr>
    <% end %>
  </tbody>

</table>