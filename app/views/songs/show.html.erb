<h1>Song Details</h1>

<table>
    <tr>
        <th>Song Title</th>
        <th>Artist</th>
        <th>Genre</th>
        <th>Released</th>
        <th>Release Year</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    <tr>
        <td><%= @song.title %></td>
        <td><%= @song.artist_name %></td>
        <td><%= @song.genre %></td>
        <td><%= @song.released %></td>
        <td><%= @song.release_year %></td>
        <td><%= link_to "Edit", edit_song_path(@song) %></td>
        <td><%= button_to "Delete", song_path(@song), method: :delete, data: {confirm: "Are you sure?"} %></td>
    </tr>
    
</table>
