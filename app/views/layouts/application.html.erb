<!DOCTYPE html>
<html>
<head>
  <title>CrudWithValidationsLab</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if @song %>
  <% if @song.errors %>
    <% @song.errors.full_messages.each do |message| %>
      <p><%= message %></p>
    <% end %>
  <% end %>
<% end %>


<%= yield %>

</body>
</html>
